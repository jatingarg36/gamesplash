<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane style="-fx-background-color: #131314;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="10.0" style="-fx-background-color: transparent;" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <AnchorPane fx:id="challenge_head_anchor" style="-fx-background-color: #3B3B3B; -fx-background-radius: 15;">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <VBox alignment="TOP_CENTER" fillWidth="false" spacing="5.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
                     <children>
                        <Label text="Invite player with this code" textFill="WHITE">
                           <font>
                              <Font name="Raleway SemiBold" size="11.0" />
                           </font>
                        </Label>
                        <TextField alignment="CENTER" editable="false" focusTraversable="false" prefColumnCount="8" promptText="Code" text="12345678">
                           <font>
                              <Font name="Raleway ExtraBold" size="18.0" />
                           </font>
                           <cursor>
                              <Cursor fx:constant="TEXT" />
                           </cursor>
                        </TextField>
                     </children>
                  </VBox>
                  <Label text="Challenge" textFill="WHITE" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
                     <font>
                        <Font name="Raleway Black" size="24.0" />
                     </font>
                  </Label>
                  <HBox fx:id="head_hbox" alignment="CENTER" spacing="20.0" AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="100.0">
                     <children>
                        <Button fx:id="create" alignment="CENTER" mnemonicParsing="false" prefHeight="45.0" prefWidth="180.0" style="-fx-background-color: #131314; -fx-background-radius: 0;" text="Create" textFill="WHITE">
                           <font>
                              <Font name="Raleway ExtraBold" size="15.0" />
                           </font>
                           <effect>
                              <DropShadow color="#0000006d" spread="0.2" />
                           </effect>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button fx:id="join" alignment="CENTER" mnemonicParsing="false" prefHeight="45.0" prefWidth="180.0" style="-fx-background-color: #131314; -fx-background-radius: 0;" text="Join" textFill="WHITE">
                           <font>
                              <Font name="Raleway ExtraBold" size="15.0" />
                           </font>
                           <effect>
                              <DropShadow color="#0000006d" spread="0.2" />
                           </effect>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button fx:id="leave" alignment="CENTER" mnemonicParsing="false" prefHeight="35.0" prefWidth="160.0" style="-fx-background-color: #CE2525; -fx-background-radius: 0;" text="Leave" textFill="WHITE">
                           <font>
                              <Font name="Raleway ExtraBold" size="15.0" />
                           </font>
                           <effect>
                              <DropShadow color="#00000065" height="20.0" radius="9.5" spread="0.2" width="20.0" />
                           </effect>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button fx:id="ready" alignment="CENTER" mnemonicParsing="false" prefHeight="35.0" prefWidth="160.0" style="-fx-background-color: #2E82DC; -fx-background-radius: 0;" text="Ready" textFill="WHITE">
                           <font>
                              <Font name="Raleway ExtraBold" size="15.0" />
                           </font>
                           <effect>
                              <DropShadow color="#00000065" height="20.0" radius="9.5" spread="0.2" width="20.0" />
                           </effect>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <ComboBox fx:id="difficulty" prefHeight="45.0" prefWidth="200.0" promptText="Choose Difficulty" style="-fx-background-color: white; -fx-background-radius: 0; -fx-font: 15 &quot;Raleway SemiBold&quot;;" />
                        <TextField fx:id="code_field" prefHeight="45.0" prefWidth="200.0" promptText="Enter Code" style="-fx-background-color: white; -fx-background-radius: 0;">
                           <font>
                              <Font name="Raleway ExtraBold" size="15.0" />
                           </font>
                        </TextField>
                        <Label fx:id="cancel" text="Cancel" textFill="WHITE">
                           <font>
                              <Font name="Raleway Medium" size="12.0" />
                           </font>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
            <AnchorPane style="-fx-background-color: #3B3B3B; -fx-background-radius: 15;">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
